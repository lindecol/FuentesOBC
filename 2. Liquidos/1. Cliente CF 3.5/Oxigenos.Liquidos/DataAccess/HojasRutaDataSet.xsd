          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="ReporteHojaRutaTableAdapter" GeneratorDataComponentClassName="ReporteHojaRutaTableAdapter" Name="ReporteHojaRuta" UserDataComponentName="ReporteHojaRutaTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="LiquidosConnectionString" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT     Productos.Codigo, Productos.Descripcion, Productos.CoeficienteProducto, Productos.UnidadMedidaVenta, Productos.Principal, 
                      HojasRuta.CodLugarCargue, HojasRuta.PesoInicial, HojasRuta.PesoFinal, HojasRuta.FechaCierre, HojasRuta.KilometrajeIniicial, 
                      HojasRuta.CodLugarDescargue, HojasRuta.PesoCierre, HojasRuta.KilometrajeFinal, HojasRuta.CodConductor, HojasRuta.CodProducto, 
                      HojasRuta.LLegadaInicioRuta, HojasRuta.SalidaInicioRuta, HojasRuta.NumRemision, HojasRuta.Cabezote, HojasRuta.Tanquero, HojasRuta.Estado, 
                      HojasRuta.IdHojaRuta, HojasRuta.PreRemision, Choferes.Nombre AS NombreChofer1, Choferes.Cedula AS cedula, Parametros.DiaSemana, 
                      Parametros.RutaPrincipal, SUBSTRING(Tanqueros.Descripcion, 10, 50) AS NombreEmpresaTrasportadora, LugaresCarga.Descripcion AS Lugarcarga, 
                      Parametros.NombreChofer, Parametros.CodigoPuntoVenta, HojasRuta.SalidaInicioRuta AS mes, HojasRuta.SalidaInicioRuta AS dia
FROM         HojasRuta INNER JOIN
                      Productos ON RTRIM(LTRIM(HojasRuta.CodProducto)) = RTRIM(LTRIM(Productos.Codigo)) INNER JOIN
                      Choferes ON HojasRuta.CodConductor = RTRIM(LTRIM(Choferes.Codigo)) INNER JOIN
                      LugaresCarga ON RTRIM(LTRIM(HojasRuta.CodLugarCargue)) = RTRIM(LTRIM(LugaresCarga.Codigo)) INNER JOIN
                      Tanqueros ON Tanqueros.Codigo = HojasRuta.Tanquero, Parametros
WHERE     (HojasRuta.IdHojaRuta IN
                          (SELECT     IdHojaRuta
                            FROM          HojasRuta HojasRuta_2
                            WHERE      (Estado = 3) AND (FechaCierre IN
                                                       (SELECT     MAX(FechaCierre) AS EXPR1
                                                         FROM          HojasRuta HojasRuta_1))))</CommandText>
