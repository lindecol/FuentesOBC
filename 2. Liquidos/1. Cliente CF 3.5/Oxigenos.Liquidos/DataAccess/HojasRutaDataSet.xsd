                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT 
                         MaestroFacturas.NoFactura, DetallePedido.Kilometraje, Clientes.Nombre AS ClienteNombre, Clientes.DiaRuta, Clientes.Direccion AS DireccionCliente, 
                         CASE DetallePedido.NivelInicial WHEN 0 THEN DetallePedido.pesoInicial ELSE DetallePedido.NivelInicial END AS NivelInicial, 
                         CASE DetallePedido.NivelFinal WHEN 0 THEN DetallePedido.pesoFinal ELSE DetallePedido.NivelFinal END AS NivelFinal, DetallePedido.PresionInicial, DetallePedido.PresionFinal, DetallePedido.Cantidad, 
                         MaestroFacturas.Fecha, MaestroFacturas.EstadoFactura, DetalleFactura.CodProducto, Pedidos.HoraAtencion, Pedidos.HoraProgramada, DATEPART(day, MaestroFacturas.Fecha) AS dia, DATEPART(month, 
                         MaestroFacturas.Fecha) AS mes, DetallePedido.CodTanque AS tanque, MaestroFacturas.TipoFactura AS TipoDoc
FROM            MaestroFacturas INNER JOIN
                         DetalleFactura ON MaestroFacturas.TipoFactura = DetalleFactura.TipoFactura AND MaestroFacturas.NoFactura = DetalleFactura.NoFactura AND MaestroFacturas.Prefijo = DetalleFactura.Prefijo INNER JOIN
                         Pedidos ON Pedidos.NoPedido = MaestroFacturas.NoPedido INNER JOIN
                         DetallePedido ON DetalleFactura.CodProducto = DetallePedido.CodProducto AND Pedidos.NoPedido = DetallePedido.NoPedido INNER JOIN
                         Clientes ON MaestroFacturas.CodCliente = Clientes.Codigo
WHERE        (MaestroFacturas.TipoFactura = 'F' OR
                         MaestroFacturas.TipoFactura = 'B') AND (MaestroFacturas.EstadoFactura = 'E') AND (DetalleFactura.CodProducto LIKE '0%') AND (MaestroFacturas.NoViaje IN
                             (SELECT        IdHojaRuta
                               FROM            HojasRuta
                               WHERE        (Estado = 3) AND (FechaCierre IN
                                                             (SELECT        MAX(FechaCierre) AS Expr1
                                                               FROM            HojasRuta AS HojasRuta_1))))</CommandText>
