          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="DocumentosGeneradosTableAdapter" GeneratorDataComponentClassName="DocumentosGeneradosTableAdapter" Name="DocumentosGenerados" UserDataComponentName="DocumentosGeneradosTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="OxigenosConnectionString" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        DOC.Abrev AS Tipo, MaestroGuias.PrefijoFactura AS prefijo, MaestroGuias.NoFactura AS DOCUMENTO, MaestroGuias.CodCliente AS CLIENTE, CLI.Nombre, CASE WHEN (CODTIPOPAGO = '01' AND TIPOFACTURA = 'F') 
                         THEN ISNULL(MONTOFACTURA, 0) ELSE 0 END AS CONTADO, CASE WHEN (CODTIPOPAGO = '02' AND TIPOFACTURA = 'F') THEN ISNULL(MONTOFACTURA, 0) ELSE 0 END AS CREDITO, 'E' AS ESTADO, DOC.Sigla, 
                         ISNULL(MaestroGuias.Reimpresion, 0) AS Reimpresion
FROM            MaestroGuias LEFT OUTER JOIN
                         MaestroFacturas AS MAE ON MaestroGuias.NoFactura = MAE.NoFactura INNER JOIN
                         Clientes AS CLI ON MaestroGuias.CodCliente = CLI.Codigo INNER JOIN
                         Documentos AS DOC ON MaestroGuias.TipoDocumento = DOC.Sigla
UNION ALL
SELECT        Documentos.Abrev AS TIPO, DEP.NoPrefijo AS PREFIJO, DEP.NoDocumento AS DOCUMENTO, DEP.CodCliente AS CLIENTE, CLI.Nombre, CASE WHEN (INDANULACION = 1) THEN 0 ELSE MONTO END AS CONTADO, 
                         0 AS CREDITO, CASE WHEN (INDANULACION = 1) THEN 'A' ELSE 'E' END AS ESTADO, Documentos.Sigla, MaestroGuias_1.Reimpresion
FROM            DepositosGarantia AS DEP INNER JOIN
                         Clientes AS CLI ON DEP.CodCliente = CLI.Codigo INNER JOIN
                         MaestroGuias AS MaestroGuias_1 ON DEP.NoDocumento = MaestroGuias_1.NoFactura LEFT OUTER JOIN
                         Talonarios ON DEP.NoPrefijo = Talonarios.Prefijo LEFT OUTER JOIN
                         Documentos ON Talonarios.CodTipoDocumento = Documentos.CodTipoDocumento
ORDER BY tipo</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
